  <owl:Class rdf:ID="Headstone">
   <rdfs:label xml:lang="en">Headstone</rdfs:label> 
   <rdfs:label xml:lang="fr">Pierre Tombale</rdfs:label>
   <rdfs:label xml:lang="de">Grabstein</rdfs:label>
   <skos:definition xml:lang="en">A stone or wooden marker used to locate a grave and placed at the top (head) of the deceased's remains.</skos:definition>
   <skos:definition xml:lang="fr">Une pierre ou un marqueur en bois utilisé pour localiser une tombe et placé au sommet (tête) de la dépouille du défunt.</skos:definition>
   <skos:definition xml:lang="de">Ein Stein- oder Holzmarker, der verwendet wird, um ein Grab zu finden und der oben (Kopf) der Überreste des Verstorbenen platziert wird.</skos:definition>
   <owl:subClassOf rdf:resource="#Tombstone" />
   <owl:disjointWith rdf:resource="#Footstone"/>
   <vs:term_status>stable</vs:term_status> 
   <rdfs:isDefinedBy rdf:resource="https://rdf.muninn-project.org/ontologies/graves-ONTOLOGY_DATE"/>
   <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
   <void:inDataset rdf:resource="#datasetdefinition"/>
  </owl:Class>

